---
---
[
  {% for post in site.posts %}{
      "type": "post",
      "title": "{{ post.title }}",
      "category": "{{ post.category }}",
      "tags": [
        {% for tag in post.tags %}"{{tag}}"{% unless forloop.last %},{% endunless %}{% endfor %}
      ],
      "url": "{{site.url}}{{ post.url }}"
    },
  {% endfor %}
  {% for page in site.pages %}{
    "type": "page",
    "title": "{{ page.title }}",
    "url": "{{ page.url }}"
  }{% unless forloop.last %},{% endunless %}{% endfor %}
]